{
  "name": "mssql-mcp-server",
  "displayName": "MSSQL MCP Server",
  "description": "A VS Code extension to start the MSSQL MCP Server for natural language database interaction.",
  "version": "1.1.0",
  "publisher": "your-publisher-id",
  "engines": {
    "vscode": ">=1.70.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:mssql-mcp-server.startServer"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mssql-mcp-server.startServer",
        "title": "Start MSSQL MCP Server"
      }
    ]
  },
  "bin": {
    "mssql-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "build:prod": "npm run build && npm prune --production"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/mssql": "^9.1.7",
    "@types/node": "^18.19.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.7",
    "shx": "^0.3.4",
    "typescript": "^5.5.0",
    "@types/vscode": "^1.70.0",
  "vsce": "^2.19.0"
  },
  "dependencies": {
    "@azure/identity": "^4.8.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^10.0.0",
    "mssql": "^11.0.1"
  },
  "type": "module"
}
